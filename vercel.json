{
  "rewrites": [
    {
      "source": "/propertymanager",
      "destination": "/index.html"
    },
    {
      "source": "/propertymanager/",
      "destination": "/index.html"
    },
    {
      "source": "/propertymanager/:path*",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/propertymanager",
      "permanent": false
    }
  ]
}
